{
  "name": "ngx-json",
  "description": "Dynamic Forms using Angular",
  "version": "0.2.0",
  "license": "MIT",
  "author": {
    "email": "cossth@gmail.com",
    "name": "Shubham Sharma",
    "url": "https://github.com/cossth"
  },
  "repository": {
    "url": "https://github.com/cossth/ngx-json.git",
    "type": "git"
  },
  "keywords": [
    "ng",
    "angular",
    "angular dynamic forms",
    "dynamic forms"
  ],
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "lint": "nx workspace-lint && ng lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "copy:readme": "cpr README.md dist/lib/ngx-json/README.md --overwrite",
    "copy:license": "cpr LICENSE dist/lib/ngx-json/LICENSE --overwrite",
    "copy:changelog": "cpr LICENSE dist/lib/ngx-json/Changelog.md --overwrite",
    "copy-files": "npm run copy-readme && npm run copy-license && npm run copy-changelog",
    "bump-version:major": "npm version major --force && cd libs/ngx-json && npm version major --force",
    "bump-version:minor": "npm version minor --force && cd libs/ngx-json  && npm version minor --force",
    "bump-version:patch": "npm version patch --force && cd libs/ngx-json && npm version patch --force",
    "build:lib-pack": "npm run bump-version:patch && nx format:write && ng build --project ngx-json --prod && npm run copy-files &&  cd dist/libs/ngx-json && npm pack && npm publish",
    "publish-library": ""
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^10.0.0",
    "@angular/common": "^10.0.0",
    "@angular/compiler": "^10.0.0",
    "@angular/core": "^10.0.0",
    "@angular/forms": "^10.0.0",
    "@angular/localize": "^10.0.0",
    "@angular/platform-browser": "^10.0.0",
    "@angular/platform-browser-dynamic": "^10.0.0",
    "@angular/router": "^10.0.0",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@nrwl/angular": "10.0.10",
    "bootstrap": "^4.5.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.7",
    "@angular-devkit/build-ng-packagr": "~0.1000.7",
    "@angular/cli": "~10.0.7",
    "@angular/compiler-cli": "^10.0.0",
    "@angular/language-service": "^10.0.0",
    "@nrwl/cypress": "10.0.10",
    "@nrwl/jest": "10.0.10",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "10.0.10",
    "@types/jest": "25.1.4",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cpr": "^3.0.1",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "ng-packagr": "^10.0.0",
    "prettier": "2.0.4",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.9.3"
  }
}
